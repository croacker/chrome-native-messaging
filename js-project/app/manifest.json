{
  "manifest_version": 2,
  "key": "",
  "name": "Chrome CROC Native Messaging",
  "version": "1.0",
  "description": "Send a message to a native application",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "CROC Native connection plugin",
    "default_icon": "icons/icon48.png"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "http://127.0.0.1:8082/*",
    "file:///*",
    "tabs",
    "nativeMessaging"
  ]
}